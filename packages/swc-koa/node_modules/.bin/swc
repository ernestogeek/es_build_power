#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tien-duy.nguyen/Dropbox/Mac/Desktop/git-perso/esbuild-swc-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/tien-duy.nguyen/Dropbox/Mac/Desktop/git-perso/esbuild-swc-demo/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@swc+cli@0.1.57_@swc+core@1.2.204/node_modules/@swc/cli/bin/swc.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@swc+cli@0.1.57_@swc+core@1.2.204/node_modules/@swc/cli/bin/swc.js" "$@"
fi
