{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start:dev": "ts-node-dev --respawn --transpile-only  -r tsconfig-paths/register ./src/main.ts",
    "start:debug": "ts-node-dev --inspect --respawn --transpile-only -r tsconfig-paths/register ./src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:e2e": "yarn setup:db && jest --config ./test/e2e/jest-e2e.json",
    "seed": "ts-node -r tsconfig-paths/register ./src/seed.ts",
    "migration": "ts-node -r tsconfig-paths/register ./src/database/migration.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express-rate-limit": "^5.2.6",
    "express-session": "^1.17.1",
    "helmet": "^4.5.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}